{
	"env": {
		"es2021": true,
		"node": true
	},
	"extends": "google",
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"quotes": "off",
		"object-curly-spacing": ["error", "always"],
		"require-jsdoc": "off",
		"no-mixed-spaces-and-tabs": "off",
		"no-tabs": "off"
	}
}
